{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "Fenrir.Git/bin/Release/*/publish/Fenrir.Git.dll"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**",
          "*.namespace.md"
        ]
      }
    ],
    "overwrite": [
      "*.namespace.md"
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Fenrir",
      "_appTitle": "Fenrir",
      "_enableSearch": true,
      "pdf": false
    }
  }
}
